<% if (errors.length > 0) { %>
<div class="container">
  <div class="row">
    <div class="alert alert-danger">
      <% errors.forEach(error => { %> <%- error %><br />
      <% }) %>
    </div>
  </div>
</div>
<% } %>
<% if (sucess.length > 0) { %>
<div class="container">
  <div class="row">
    <div class="alert aler-sucess">
      <%- sucess %>
    </div>
  </div>
</div>
<% } %>
